{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
			"import/no-unresolved":"off",
			"import/extensions":"off",
			"import/prefer-default-export":"off",
			"camelcase":"off",
			"no-tabs":"off",
			"no-mixed-spaces-and-tabs":"off",
			"no-unused-vars":"off",
			"semi":"off",
			"max-len":"off"
    }
}
{
   "type": "mongodb",
   "host": "localhost",
   "port": 27017,
   "database": "teste",
   "synchronize": false,
   "logging": false,
   "entities": [
      "src/app/models/**/*.ts"
   ],
   "migrations": [
      "src/database/migrations/**/*.ts"
   ],
   "subscribers": [
      "src/subscriber/**/*.ts"
   ],
   "cli": {
      "entitiesDir": "src/app/models",
      "migrationsDir": "src/database/migrations",
      "subscribersDir": "src/subscriber"
   }
}
